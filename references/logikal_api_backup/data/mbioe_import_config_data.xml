<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <!-- Default MBIOE Import Configuration -->
    <record id="default_mbioe_import_config" model="mbioe.import.config">
        <field name="name">Default MBIOE Import Configuration</field>
        <field name="active" eval="True"/>
        <field name="auto_create_products" eval="True"/>
        <field name="product_name_template">Elevation - {elevation_type}</field>
        <field name="product_code_template">ELEV_{elevation_type}_{identifier}</field>
        <field name="use_mbioe_pricing" eval="True"/>
        <field name="default_price_when_missing">0.0</field>
        <field name="clear_existing_on_reimport" eval="True"/>
        <field name="section_name_template">Phase: {phase_name}</field>
        <field name="elevation_description_template">{elevation_name}
Type: {elevation_type}
Dimensions: {width}mm x {height}mm
Area: {area}mÂ²</field>
        <field name="import_sequence_increment">10</field>
    </record>
    
</odoo>
